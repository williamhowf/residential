<div class="panel-group">
    @await Component.InvokeAsync("AdminWidget", new { widgetZone = "admin_videoguides_details_info_top", additionalData = Model.Id })
    <div class="panel panel-default">
        <div class="panel-body">

            <div class="form-group">
                <div class="col-md-3">
                    <nop-label asp-for="ContentTittleChinese" />
                </div>
                <div class="col-md-9">
                    <nop-editor asp-for="ContentTittleChinese" asp-required="true" />
                    <span asp-validation-for="ContentTittleChinese"></span>
                </div>
            </div>
        </div>
    </div>
    @await Component.InvokeAsync("AdminWidget", new { widgetZone = "admin_videoguides_details_info_bottom", additionalData = Model.Id })
</div>
